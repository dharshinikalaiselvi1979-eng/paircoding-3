def is_sorted(nums):

    is_increasing = True
    is_decreasing = True


    for i in range(len(nums) - 1):
        if nums[i] > nums[i + 1]:
            is_increasing = False
        if nums[i] < nums[i + 1]:
            is_decreasing = False


    return is_increasing or is_decreasing


nums =list(map(int,input().split()))
print(is_sorted(nums))
